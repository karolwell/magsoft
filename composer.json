{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-export": "^1.2",
        "yiisoft/yii2-httpclient": "^2.0",
        "drmabuse/yii2-slick-carousel": "dev-master",
        "maybeworks/yii2-owl.carousel": "*",
        "kartik-v/yii2-mpdf": "^1.0",
        "kartik-v/yii2-icons": "dev-master",
        "yiisoft/yii2-apidoc": "~2.0.0",
        "demogorgorn/yii2-ajax-submit-button": "*",
        "karnbrockgmbh/yii2-modal-ajax": "@dev",
        "kartik-v/yii2-social": "dev-master",
        "marciocamello/yii2-paypal": "*",
        "kun391/yii2-paypal": "*",
        "kartik-v/yii2-popover-x": "*",
        "phpnt/yii2-export": "^0.0.2",
        "2amigos/yii2-google-maps-library": "*",
        "2amigos/yii2-highcharts-widget" : "*",
        "yii2mod/yii2-google-maps-markers": "^1.3",
        "2amigos/egmap": "dev-master",
        "voodoo-mobile/yii2-cron": "^1.0",
        "yiisoft/yii2-twig": "^2.1",
        "linslin/yii2-curl": "*"
        
    },
    "require-dev": {
        "php": ">=5.4.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
